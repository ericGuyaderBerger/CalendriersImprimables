{
  "name": "roche-fils-calendriers-solution",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "vue-serve": "vue-cli-service serve",
    "vue-build": "vue-cli-service build",
    "vue-lint": "vue-cli-service lint",
    "electron": "electron electron-start",
    "dev": "concurrently --kill-others \"yarn vue-serve\" \"sleep 15 && cross-env NODE_ENV=DEV yarn electron\"" 
  },
  "dependencies": {
    "electron": "^2.0.6",
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.15",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.15",
    "@vue/cli-service": "^3.0.0-beta.15",
    "concurrently": "^3.6.1",
    "cross-env": "^5.2.0",
    "vue-template-compiler": "^2.5.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
